        MENUITEM "Nom avant filtrage\t$(UnfilteredName)", ID_REPLACE_UNFILTEREDNAME
        MENUITEM "Nom après filtrage\t$(FilteredName)", ID_REPLACE_FILTEREDNAME
        MENUITEM SEPARATOR
        POPUP "Lecteur de Tag ID3"
        BEGIN
            MENUITEM "Piste\t$(ID3Track)",          ID_ID3TAGREADER_TRACK
            MENUITEM "Artiste\t$(ID3Artist)",       ID_ID3TAGREADER_ARTIST
            MENUITEM "Titre\t$(ID3Title)",          ID_ID3TAGREADER_TITLE
            MENUITEM "Album\t$(ID3Album)",          ID_ID3TAGREADER_ALBUM
            MENUITEM "Année\t$(ID3Year)",           ID_ID3TAGREADER_YEAR
            MENUITEM "Commentaire\t$(ID3Comment)",  ID_ID3TAGREADER_COMMENT
            MENUITEM "Genre\t$(ID3Genre)",          ID_ID3TAGREADER_GENRE
        END
        MENUITEM SEPARATOR
        POPUP "Expresionns Régulières"
        BEGIN
            MENUITEM "Rechercher\t\\0",             ID_REGEXP_FINDWHATTEXT
            MENUITEM "Expression avec balises 1\t\\1", ID_REGEXP_TAGGEDEXPRESSION1
            MENUITEM "Expression avec balises 2\t\\2", ID_REGEXP_TAGGEDEXPRESSION2
            MENUITEM "Expression avec balises 3\t\\3", ID_REGEXP_TAGGEDEXPRESSION3
            MENUITEM "Expression avec balises 4\t\\4", ID_REGEXP_TAGGEDEXPRESSION4
            MENUITEM "Expression avec balises 5\t\\5", ID_REGEXP_TAGGEDEXPRESSION5
            MENUITEM "Expression avec balises 6\t\\6", ID_REGEXP_TAGGEDEXPRESSION6
            MENUITEM "Expression avec balises 7\t\\7", ID_REGEXP_TAGGEDEXPRESSION7
            MENUITEM "Expression avec balises 8\t\\8", ID_REGEXP_TAGGEDEXPRESSION8
            MENUITEM "Expression avec balises 9\t\\9", ID_REGEXP_TAGGEDEXPRESSION9
        END
        POPUP "Wildcards"
        BEGIN
            MENUITEM "Tagget expression (characters matching '*')\t*", ID_TAGGETBYSTAR
        END
    END
