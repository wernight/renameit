BEGIN
    POPUP "Replace macros"
    BEGIN
        MENUITEM "Répertoire du fichier\t$(FileDir)", ID_REPLACE_FILEDIR
        MENUITEM "Nom du fichier\t$(FileName)", ID_REPLACE_FILENAME
