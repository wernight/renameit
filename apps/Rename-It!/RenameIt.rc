        MENUITEM "Enregistrer sous...",         ID_FILE_SAVEAS
        MENUITEM SEPARATOR
        MENUITEM "&Configurer",                 ID_FILE_CONFIGURE
        MENUITEM SEPARATOR
        MENUITEM "&Quitter",                    ID_FILE_EXIT
    END
    POPUP "Fe&nêtre"
    BEGIN
        MENUITEM "&Toujours au dessus",         ID_VIEW_ALWAYS_ON_TOP
    END
    POPUP "&?"
    BEGIN
        MENUITEM "&Aide\tF1",                   ID_HELP_INDEX
        MENUITEM "&Qu'est-ce que c'est?\tMaj+F1", ID_CONTEXT_HELP
        POPUP "Feedback"
        BEGIN
            MENUITEM "Rapporter un bogue",          ID_FEEDBACK_BUGREPORT
            MENUITEM "Demande d'information",       ID_FEEDBACK_INFORMATIONREQUEST
            MENUITEM "Suggestions",                 ID_FEEDBACK_SUGGESTIONS
        END
        MENUITEM SEPARATOR
        MENUITEM "&A propos",                   ID_HELP_ABOUT
    END
END

IDR_SEARCH_WILD MENU 
BEGIN
    POPUP "Search Wildcard"
    BEGIN
        MENUITEM "Zéro ou plusieurs caractères\t*", ID_WILD_0ORMORE
        MENUITEM "Tout caractère unique\t?",    ID_WILD_1MATCH
    END
END

IDR_SEARCH_REGEXP MENU 
BEGIN
    POPUP "Search RegExp"
    BEGIN
        MENUITEM "Tout caractère unique\t.",    ID_ANYCHARACTER
        MENUITEM "Zéro ou plus\t*",             ID_0ORMOREMATCHES
        MENUITEM "Un ou plus\t+",               ID_1ORMOREMATCHES
        MENUITEM SEPARATOR
        MENUITEM "Début de ligne\t^",           ID_BEGINNGINGOFLINE
        MENUITEM "Fin de ligne\t$",             ID_ENDOFLINE
        MENUITEM "Saut de ligne\t\\n",          ID_SEARCHREGEXP_NEWLINE
        MENUITEM SEPARATOR
        MENUITEM "Tout caractère de l'ensemble\t[ ]", ID_CHARACTERINRANGE
        MENUITEM "Tout caractère hors de l'ensemble\t[^ ]", ID_CHARACTERNOTINRANGE
        MENUITEM "Ou\t|",                       ID_OR
        MENUITEM "Expression de balise\t( )",   ID_TAGGEDEXPRESSION
        MENUITEM SEPARATOR
        MENUITEM "Caractère alphanumerique\t[0-9A-Za-z]", ID_ALPHANUMERICCHARACTER
        MENUITEM "Caractère de l'alphabet\t[A-Za-z]", ID_ALPHABETICCHARACTER
        MENUITEM "Chiffre\t[0-9]",              ID_DECIMALDIGIT
    END
END

